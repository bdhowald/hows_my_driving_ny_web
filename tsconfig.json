{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "baseUrl": "src",
    "forceConsistentCasingInFileNames": true,
    "lib": [
      "dom",
      "es2017"
    ],
    "moduleResolution": "Node",
    "noEmit": true,
    "noUnusedLocals": false,
    "noUnusedParameters": true,
    "strict": true,
    "target": "es6",
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "module": "esnext",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "react"
  },
  "target": "ES6",
  "include": [
    "src"
  ]
}
